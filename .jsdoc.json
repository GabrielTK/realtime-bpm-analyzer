{
	"plugins": [],
	"recurseDepth": 1,
	"source": {
		"include": [
			"./src"
		],
		"includePattern": ".(js)$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./public/api",
		"template": "node_modules/clean-jsdoc-theme",
		"meta": [{
			"name": "author",
			"content": "David Lepaux"
		}, {
			"name": "description",
			"content": "Library to analyze BPM (Tempo) of a song in realtime on your browser"
		}],
		"theme_opts": {
            "theme": "light",
			"menu" : [{
				"title": "Back to documentation",
				"link": "https://dlepaux.github.io/realtime-bpm-analyzer/"
			}],
			"add_script_path": [{
				"href": "https://www.googletagmanager.com/gtag/js?id=G-PXQ4D05F0B"
			}],
			"add_scripts": "window.dataLayer = window.dataLayer || []; function gtag() {dataLayer.push(arguments)} gtag('js', new Date()); gtag('config', 'G-PXQ4D05F0B');",
			"add_style_path": [{
				"rel": "icon",
				"href": "https://dlepaux.github.io/realtime-bpm-analyzer/favicon.png"
			}]
        },
		"search": {
			"shouldSort": true,
			"threshold": 0.4,
			"location": 0,
			"distance": 100,
			"maxPatternLength": 32,
			"minMatchCharLength": 1
		},
		"create_style": "nav{background: yellow} footer {background: green}"
	}
}
